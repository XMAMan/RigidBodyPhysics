<UserControl x:Class="EditorControl.View.RotaryMotorPropertyControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EditorControl.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Content="RotaryForce"/>
            <TextBox Text="{Binding Path=RotaryForce, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged,StringFormat=N6}"/>
        </StackPanel>
        <CheckBox IsChecked="{Binding IsEnabled}" Content="IsEnabled"/>
    </StackPanel>
</UserControl>
